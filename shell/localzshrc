alias encryptproxpass='cd ~/src/proximate_secure && \
                       gpg -e --armor --sign -o proximate_login_info.gpg \
                       -r $SOFIA_KEY \
                       -r $EVAN_KEY \
                       -r $WEI_KEY \
                       -r $ROBB_KEY \
                       tmp.md && \
                       rm -f tmp.md'

